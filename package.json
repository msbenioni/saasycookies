{
  "name": "nz-invoice-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite",
    "start:analytics": "node server/analytics-api.js",
    "start:all": "powershell -Command \"Start-Process powershell -ArgumentList '-NoExit', '-Command', 'cd $PWD; npm run start:analytics' ; npm start\"",
    "netlify:dev": "netlify dev",
    "netlify:build": "npm run build && npm run copy-functions",
    "copy-functions": "powershell -Command \"Copy-Item -Path ./node_modules/@google-analytics -Destination ./netlify/functions/node_modules/@google-analytics -Recurse -Force\""
  },
  "dependencies": {
    "@google-analytics/data": "^5.1.0",
    "@hookform/resolvers": "^3.3.4",
    "@types/recharts": "^1.8.29",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.4.0",
    "recharts": "^2.15.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "netlify-cli": "^16.4.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.2.3"
  }
}
